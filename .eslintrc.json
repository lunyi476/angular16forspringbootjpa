{
  "env": {
    "browser": true,
    "es2021": true
  },

  "overrides": [
    { 
      "files": ["*.ts", "*.js"],
      "extends": [
          "eslint:recommended",
          "plugin:@typescript-eslint/recommended"
      ],

     "parser": "@typescript-eslint/parser",    
  
     "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
       },
    
    
    "plugins": [
        "@typescript-eslint", "@angular-eslint"
       ],
    "rules": {
        "@angular-eslint/no-empty-lifecycle-method": "off"
      }
    }

  ]
}
 